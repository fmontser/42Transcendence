{
	"tables": [
		"CREATE TABLE IF NOT EXISTS users (id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT UNIQUE NOT NULL, pass TEXT NOT NULL)"
	],

	"endpoints": [
		{
			"GET": [
				{
					"getUsers": [{
						"path": "/database/front/get/users",
						"sql": "SELECT * FROM users"
					}]
				}
			],

			"POST": [
				{
					"postUser": [{
						"path": "/database/front/post/user",
						"args": "user, pass"
					}]
				}
			]
		}
	]
}